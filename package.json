{
  "name": "csx",
  "version": "8.5.0",
  "description": "Utility functions for TypeStyle",
  "main": "./lib/index.js",
  "module": "./lib.es2015/index.js",
  "jsnext:main": "./lib.es2015/index.js",
  "typings": "./lib/index",
  "types": "./lib/index",
  "scripts": {
    "build": "tsc -p . && tsc -p tsconfig.es2015.json && webpack",
    "clean": "node_modules/.bin/del-cli -f lib lib.es2015 umd",
    "prebuild": "npm run clean",
    "test": "node_modules/.bin/mocha --compilers ts:ts-node/register ./tests/**/**.ts",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags && npm publish",
    "u": "npm install typestyle@latest --save-exact --save-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typestyle/csx.git"
  },
  "keywords": [
    "TypeScript",
    "JSX",
    "TSX",
    "Layout"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typestyle/csx/issues"
  },
  "homepage": "https://github.com/typestyle/csx#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.34",
    "del-cli": "^1.1.0",
    "mocha": "^4.0.1",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3",
    "typestyle": "^1.4.2",
    "uglify-loader": "^2.0.0",
    "webpack": "^3.7.1"
  }
}
