{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "jsx": "react"
    },
    "include": [
        "lib/**/*",
        "demo/**/*"
    ],
    "compileOnSave": false
}
